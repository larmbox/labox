{
  "name": "@larmbox/labox",
  "version": "0.0.22",
  "description": "Labox is a component library.",
  "license": "MIT",
  "main": "dist/labox.umd.js",
  "module": "dist/labox.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/labox.umd.js",
      "import": "./dist/labox.es.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/": {
      "require": "./dist/",
      "import": "./dist/"
    },
    "./nuxt/": {
      "require": "./nuxt/dist/module.cjs",
      "import": "./nuxt/dist/module.mjs",
      "types": "./nuxt/dist/types.d.ts"
    }
  },
  "files": [
    "dist",
    "nuxt/dist"
  ],
  "web-types": "dist/web-types.json",
  "vetur": {
    "tags": "dist/vetur-tags.json",
    "attributes": "dist/vetur-attributes.json"
  },
  "sideEffects": false,
  "scripts": {
    "build-nuxt-module": "nuxt-module-build ./nuxt",
    "build": "cross-env NODE_ENV=production ts-node build/pre-build && vite build --config build/vite.config.js && tsc --p tsconfig.build.json --declaration --emitDeclarationOnly && tsc-alias -p tsconfig.build.json && npm run build:sass && ts-node -r tsconfig-paths/register build/post-build",
    "build:sass": "sass ./dist:./dist",
    "build:vuepress": "vuepress build docs",
    "deploy": "build/deploy-docs.sh",
    "start:dev": "vuepress dev docs",
    "publish:npm": "npm i && npm run build && npm publish --access public",
    "lint": "eslint --fix .",
    "test": "vitest --config ./build/vite.config.js",
    "coverage": "vitest run --coverage --config ./build/vite.config.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@nuxt/kit": "^3.0.0-rc.3",
    "@nuxt/module-builder": "^0.1.7",
    "@types/fs-extra": "^9.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "c8": "^7.11.2",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^8.0.1",
    "fs-extra": "^10.0.0",
    "happy-dom": "^2.34.0",
    "highlight.js": "^11.3.1",
    "nanoid": "^3.1.30",
    "nuxt": "^3.0.0-rc",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^10.2.0",
    "showdown": "^2.1.0",
    "ts-node": "^10.5.0",
    "tsc-alias": "^1.6.6",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.5.2",
    "vite": "^2.9.9",
    "vitest": "^0.3.6",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15",
    "vuelr": "^1.0.7",
    "vuepress": "^2.0.0-beta.38"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.0"
  }
}
