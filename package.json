{
  "name": "@larmbox/labox",
  "version": "0.0.17",
  "description": "Labox is a component library.",
  "license": "MIT",
  "main": "dist/labox.umd.js",
  "module": "dist/labox.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "web-types": "dist/web-types.json",
  "vetur": {
    "tags": "dist/vetur-tags.json",
    "attributes": "dist/vetur-attributes.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production node build/pre-build && vite build --config build/vite.config.js && tsc --declaration --emitDeclarationOnly && npm run build:sass && node build/post-build",
    "build:sass": "sass ./dist:./dist",
    "docs:deploy": "build/deploy-docs.sh",
    "docs:deploy:win": ".\\build\\deploy-docs.sh",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "eslint --fix .",
    "publish:npm": "npm i && npm run build && npm publish --access public"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@types/fs-extra": "^9.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^1.10.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "chalk": "^4.1.2",
    "codemirror": "^5.64.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^8.0.1",
    "fs-extra": "^10.0.0",
    "highlight.js": "^11.3.1",
    "nanoid": "^3.1.30",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^10.2.0",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vue": "^3.2.23",
    "vue-router": "^4.0.12",
    "vuelr": "^1.0.4",
    "vuepress": "^2.0.0-beta.27"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.0"
  }
}
